#!/usr/bin/env bash

cattail_self() {
    cat "$0" | after '^###' | tail -n +3
}

if [[ -z "$1" ]]; then
    cattail_self
    exit 0
fi

# Otherwise:
cattail_self | grep --color=auto -Ei "^$@.*" | awk '{print "git clone " $2}'

exit 0

